<script type="text/javascript">
	function doDisplay(){
		var element = document.getElementById('articleFactcheckInfo');
		if (element.classList.contains('is_hidden')) {
			element.classList.remove('is_hidden');
		} else {
			element.classList.add('is_hidden');
		}
	}
</script>

